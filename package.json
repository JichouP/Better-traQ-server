{
  "name": "better-traq-server",
  "version": "1.0.0",
  "description": "Server for Better traQ",
  "main": "index.js",
  "scripts": {
    "serve": "ts-node --files -r tsconfig-paths/register src/app.ts",
    "watch": "nodemon",
    "lint": "eslint src",
    "test": "jest --color",
    "testWithCoverage": "jest --collectCoverage --color",
    "openCoverage": "sensible-browser ./coverage/lcov-report/index.html",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JichouP/Better-traQ-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JichouP/Better-traQ-server/issues"
  },
  "homepage": "https://github.com/JichouP/Better-traQ-server#readme",
  "dependencies": {
    "@typegoose/typegoose": "^7.4.2",
    "express": "^4.17.1",
    "mongoose": "^5.10.18",
    "tsconfig-paths": "^3.9.0",
    "zod": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@shelf/jest-mongodb": "^1.2.4",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/mongoose": "^5.10.5",
    "@types/sinon-express-mock": "^1.3.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "sinon": "^10.0.0",
    "sinon-express-mock": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{md,html,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
